FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      physicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel Newtonian;
rho            rhoinf;        // density [kg/m^3]
mu             [1 -1 -1 0 0 0 0] 1.002e-3;  // dynamic viscosity [Pa*s]
nu             [0 2 -1 0 0 0 0] 1.005e-6;    // kinematic viscosity (mu/rho)
rhoinf         [1 -3 0 0 0 0 0] 997; // density [kg/m^3]
