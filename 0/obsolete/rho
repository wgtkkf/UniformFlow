FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 997;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 997;
    }
    
    outlet
    {
        type            calculated;
        value           uniform 997;
    }
    
    upperWall
    {
        type            calculated;
        value           uniform 997;
    }
    
    lowerWall
    {
        type            calculated;
        value           uniform 997;
    }
    
    frontAndBack
    {
        type            calculated;
        value           uniform 997;
    }
}

